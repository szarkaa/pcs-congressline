<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="2025-12-13-create-online-reg-custom-question-answers" author="aszarka">

        <preConditions onFail="MARK_RAN" onError="MARK_RAN">
            <not>
                <tableExists tableName="online_reg_custom_question_answers"/>
            </not>
        </preConditions>

        <createTable tableName="online_reg_custom_question_answers">
            <column name="online_reg_custom_question_id" type="BIGINT(20)">
                <constraints nullable="false"/>
            </column>

            <column name="value" type="VARCHAR(1000)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>
